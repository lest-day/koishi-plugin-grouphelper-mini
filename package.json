{
  "name": "koishi-plugin-grouphelper",
  "description": "带有webui的功能全面的群管插件",
  "version": "0.3.12",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "contributors": [
    "camvan <3499880997@qq.com>",
    "TianLuen <1318339790@qq.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/camvanaa/koishi-plugin-grouphelper.git"
  },
  "files": [
    "lib",
    "dist",
    "client"
  ],
  "license": "MIT",
  "scripts": {
    "build:server": "tsc",
    "build:client": "koishi-console build",
    "build": "yarn build:server && yarn build:client"
  },
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "group",
    "management",
    "moderation"
  ],
  "devDependencies": {
    "@koishijs/client": "^5.30.0",
    "@koishijs/plugin-console": "^5.30.0",
    "koishi": "^4.18.7",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@koishijs/plugin-console": "^5.0.0",
    "koishi": "^4.18.3"
  },
  "koishi": {
    "description": {
      "en": "A feature-rich group management plugin with web UI",
      "zh": "带有webui的功能全面的群管插件"
    },
    "service": {
      "required": [
        "database"
      ],
      "optional": [
        "console"
      ],
      "implements": [
        "groupHelper"
      ]
    },
    "locales": [
      "zh-CN"
    ],
    "browser": true,
    "icon": "mdi:account-group"
  }
}
